<form method="POST" action="/lookup">
  <p>Look up a phone number</p>
  <p>
    Phone Number:
    <input type="tel" name="phone" required placeholder="11234567890">
  </p>
  <p>
    Number to text the info to:
    <input type="tel" name="response" required placeholder="11234567890">
  </p>
  <input type="submit">
</form>
